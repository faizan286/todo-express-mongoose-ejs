<%- include('header'); -%>

    <div class="box" id="heading">
        <h1><%= title %></h1>
    </div>

    <div class="box">
        <% items.forEach(item => { %>
        <div class="item">
            <input type="checkbox">
            <p><%= item %></p>
        </div>
        <% }) %>
        
        <form class="item" action="/add" method="POST">
            <input type="text" name="title" placeholder="write here..." autocomplete="off">
            <button type="submit" name="list" value=<%= title %> >+</button>
        </form>
    </div>

<%- include('footer'); -%>